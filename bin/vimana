#!/usr/bin/perl
use lib 'lib';
use strict;
use warnings;
use Vimana;

use Vimana::Command;

my $cmd = shift @ARGV;
my $ret = Vimana::Command->invoke( $cmd, @ARGV );

exit (defined $ret ? $ret : 1);






